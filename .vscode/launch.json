{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Main",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node",
                "dist/server.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "PORT": "3000",
            }
        },
        {
            "name": "Run Consumer",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "ts-node",
                "dist/queue/server.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "REDIS_HOST": "127.0.0.1",
                "REDIS_PORT": "6379",
            }
        }
    ]
}